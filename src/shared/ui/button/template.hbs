<button
    class="{{#if classes}}{{classes}} {{/if}}button button__{{#if variant}}{{variant}}{{else}}filled{{/if}}"
    {{#if tabindex}}tabindex="{{tabindex}}"{{/if}}
    {{#if autofocus}}autofocus{{/if}}
    {{#if type}}type={{type}}{{/if}}
    {{#if disabled}}disabled{{/if}}
>
    {{label}}
</button>
